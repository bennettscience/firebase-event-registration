{
  "rules": {
    "courses": {
      ".read": "(auth != null && auth.token.email.matches(/.*@elkhart.k12.in.us$/))",
      "$courseId": {
        "members": {
          "$uid": {
            ".validate": "root.child('courses').child($courseId).child('code').val() == newData.child('code').val()",
            ".write": "auth != null"
          }
        }
      }
    },
    "users": {
      "$userId": {
        "regs": {
          ".read": "(auth != null && auth.uid == $userId)",
          ".write": "(auth != null && auth.uid == $userId)"
        },
        ".read": "(auth != null && auth.uid == $userId)",
        ".write": "(auth != null && auth.uid == $userId)"
      }
    },
    "device_ids": {
      ".read": "auth != null",
      ".write": "auth != null"
    }
  }
}
